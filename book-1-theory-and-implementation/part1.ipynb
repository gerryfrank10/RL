    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"class CloseFormAgent:\\n\",\n",
    "    \"    def __init__(self, _):\\n\",\n",
    "    \"        pass\\n\",\n",
    "    \"    def reset(self, mode=None):\\n\",\n",
    "    \"        pass\\n\",\n",
    "    \"    def step(self, observation, reward, terminated):\\n\",\n",
    "    \"        position, velocity = observation\\n\",\n",
    "    \"        lb = min(-0.9 * (position + 0.25) **2 + 0.03, 0.3 * (position + 0.9) **4 -0.08)\\n\",\n",
    "    \"        ub = -0.07 * (position + 0.38) ** 2 + 0.07\\n\",\n",
    "    \"        if lb < velocity < ub:\\n\",\n",
    "    \"            action = 2\\n\",\n",
    "    \"        else:\\n\",\n",
    "    \"            action = 0\\n\",\n",
    "    \"        return action\\n\",\n",
    "    \"    def close(self):\\n\",\n",
    "    \"        pass\\n\",\n",
    "    \"agent = CloseFormAgent(env)\"\n",
