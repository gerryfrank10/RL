    "   \"cell_type\": \"code\",\n",
    "   \"source\": [\n",
    "    \"def play_episode(env, agent, render_mode=None):\\n\",\n",
    "    \"    observation, info = env.reset()\\n\",\n",
    "    \"    terminated = False\\n\",\n",
    "    \"    truncated = False\\n\",\n",
    "    \"    total_reward = 0.0\\n\",\n",
    "    \"    while not (terminated or truncated):\\n\",\n",
    "    \"        if render_mode:\\n\",\n",
    "    \"            env.render()\\n\",\n",
    "    \"        action = agent.step(observation, 0.0, terminated)\\n\",\n",
    "    \"        observation, reward, terminated, truncated, info = env.step(action)\\n\",\n",
    "    \"        total_reward += reward\\n\",\n",
    "    \"    return total_reward\"\n",
